{
  "name": "mtime",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.4.5",
    "antd-mobile": "^2.1.8",
    "axios": "^0.18.0",
    "create-react-app": "^1.5.2",
    "jquery": "^3.3.1",
    "node-sass-chokidar": "^1.2.2",
    "react": "^16.3.2",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^16.3.2",
    "react-native-scrolltotop": "^0.0.6",
    "react-promise": "^2.0.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "react-swipe": "^5.1.1",
    "redux": "^4.0.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "proxy": {
    "/Service/callback.mi/": {
      "target": "https://m.mtime.cn",
      "host": "m.mtime.cn",
      "changeOrigin": true
    },
    "/Service/callback-mall.mi/": {
      "target": "http://m.mtime.cn",
      "host": "m.mtime.cn",
      "changeOrigin": true
    },
    "/api/proxy/ticket/OnlineLocationCinema/": {
      "target": "https://ticket-m.mtime.cn",
      "host": "ticket-m.mtime.cn",
      "changeOrigin": true
    },
    "/registerrequest": {
      "target": "http://localhost:8000",
      "host": "localhost:8000",
      "changeOrigin": true
    },
    "/login": {
      "target": "http://localhost:8000",
      "host": "localhost:8000",
      "changeOrigin": true
    }
  }
}
